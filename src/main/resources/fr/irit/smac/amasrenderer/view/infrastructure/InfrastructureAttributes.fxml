<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="attributesServiceDialog" prefHeight="450.0" prefWidth="630.0" styleClass="dialog" stylesheets="@../../css/dialog.css" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.irit.smac.amasrenderer.controller.infrastructure.InfrastructureAttributesController">
	<top>
		<Label prefHeight="42.0" prefWidth="630.0" styleClass="title" text="Attributs de l'infrastructure" BorderPane.alignment="CENTER" />
	</top>
	<center>
		<TreeView fx:id="tree" editable="true" focusTraversable="false" prefHeight="303.0" prefWidth="31.0" BorderPane.alignment="CENTER" />
	</center>
	<bottom>
		<AnchorPane prefHeight="70.0" prefWidth="630.0" BorderPane.alignment="CENTER">
			<children>
				<Button fx:id="confButton" layoutX="428.0" layoutY="10.0" mnemonicParsing="false" onAction="#confirmButton" prefHeight="33.0" prefWidth="106.0" text="Confirmer">
					<styleClass>
						<String fx:value="buttonDialogOk" />
						<String fx:value="buttonDialog" />
					</styleClass>
				</Button>
				<Button fx:id="cancButton" layoutX="14.0" layoutY="10.0" mnemonicParsing="false" onAction="#cancelButton" prefHeight="33.0" prefWidth="99.0" styleClass="buttonDialog" text="Annuler" />
			</children>
		</AnchorPane>
	</bottom>
</BorderPane>
