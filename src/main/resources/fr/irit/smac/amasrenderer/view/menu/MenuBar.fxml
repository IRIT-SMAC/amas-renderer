<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar fx:id="menuBar" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.irit.smac.amasrenderer.controller.menu.MenuBarController">
  <menus>
    <Menu mnemonicParsing="false" text="Fichier">
      <items>
            <MenuItem mnemonicParsing="false" onAction="#clickMenuLoad" text="Charger" />
            <MenuItem mnemonicParsing="false" onAction="#clickMenuSave" text="Enregistrer" />
            
        <MenuItem mnemonicParsing="false" onAction="#clickMenuExit" text="Fermer" />
        
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Aide">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#clickDocumentation" text="Documentation" />
      </items>
    </Menu>
  </menus>
</MenuBar>
